{
    "name": "ALICE-SaaS/visitor-management-service",
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "aws/aws-sdk-php": "^3.140",
        "doctrine/collections": "1.6.4",
        "doctrine/orm": "^2.7",
        "firebase/php-jwt": "^5.2",
        "guzzlehttp/guzzle": "~6.0",
        "monolog/monolog": "^1.24",
        "php-di/php-di": "^6.0",
        "predis/predis": "^1.1",
        "robmorgan/phinx": "^0.11.4",
        "slim/php-view": "^2.2",
        "slim/psr7": "^0.5",
        "slim/slim": "^4.1",
        "zircote/swagger-php": "^3.0"
    },
    "require-dev": {
        "phpspec/prophecy": "~1.0",
        "phpunit/phpunit": "^9.1",
        "vimeo/psalm": "^3.11"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
